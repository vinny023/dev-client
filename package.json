    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "@ui-kitten/components": "^5.0.0",
    "@ui-kitten/eva-icons": "^5.0.0",
    "axios": "^0.21.1",
