{
  "expo": {
    "name": "SupplyHero",
    "slug": "SupplyHero",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/SupplyHeroLogo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0666EB"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.getweys.truffle",
      "buildNumber": "3"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/SupplyHeroLogo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "sdkVersion": "41.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ]
  }
}
